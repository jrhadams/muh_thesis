@article{Saefken2014,
  title = {A Unifying Approach to the Estimation of the Conditional {{Akaike}} Information in Generalized Linear Mixed Models},
  author = {Saefken, Benjamin and Kneib, Thomas and family=Waveren, given=Clara Sophie, prefix=van, useprefix=true and Greven, Sonja},
  date = {2014},
  journaltitle = {Electronic Journal of Statistics},
  volume = {8},
  number = {1},
  pages = {201--225},
  issn = {19357524},
  doi = {10.1214/14-EJS881},
  abstract = {The conditional Akaike information criterion, AIC, has been frequently used for model selection in linear mixed models. We develop a general framework for the calculation of the conditional AIC for different exponential family distributions. This unified framework incorporates the conditional AIC for the Gaussian case, gives a new justification for Poisson distributed data and yields a new conditional AIC for exponentially dis- tributed responses but cannot be applied to the binomial and gamma distri- butions. The proposed conditional Akaike information criteria are unbiased for finite samples, do not rely on a particular estimation method and do not assume that the variance-covariance matrix of the random effects is known. The theoretical results are investigated in a simulation study. The practical use of the method is illustrated by application to a data set on tree growth.},
  keywords = {Conditional Akaike information criterion,Generalized linear mixed models,Kull- back-Leibler distance,Model selection,Random effects}
}

