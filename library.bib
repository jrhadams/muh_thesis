@article{Surai2001,
  title = {Designer Eggs: From Improvement of Egg Composition to Functional Food},
  author = {Surai, P.F and Sparks, N.H.C},
  date = {2001-01},
  journaltitle = {Trends in Food Science \& Technology},
  volume = {12},
  number = {1},
