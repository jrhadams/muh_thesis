@article{Ormerod2012,
  title = {Gaussian Variational Approximate Inference for Generalized Linear Mixed Models},
  author = {Ormerod, J. T. and Wand, M. P.},
  date = {2012},
  journaltitle = {Journal of Computational and Graphical Statistics},
  volume = {21},
