@article{Haverkort1990,
  title = {Ecology of Potato Cropping Systems in Relation to Latitude and Altitude},
  author = {Haverkort, A. J.},
  date = {1990-01},
  journaltitle = {Agricultural Systems},
  volume = {32},
  number = {3},
  pages = {251--272},
  issn = {0308-521X},
  doi = {10.1016/0308-521X(90)90004-A},
  url = {https://www.sciencedirect.com/science/article/pii/0308521X9090004A},
  urldate = {2023-05-06},
  abstract = {In this review the climate as affected by latitude from 0 to 50° N and altitude above sea level is described in relation to potato cultivation for the area between 20° W and 50° E. The influence of daylength, solar radiation, temperature and rainfall on the time of planting, the length of the growing season, the number of seasons per year, the source of planting material and yields of potato crops is discussed. The relationships between climate and potato production are explained in terms of present cropping patterns and constraints for the increase of yields and hectarage in the various ecological zones of which illustrative statistics concerning climate and potato crop are provided. It is shown that the potato is a versatile commodity adapted to a wide range of environments and hints are given on further exploitation of its potential in the various ecosystems.},
  langid = {english},
  file = {/home/jrha/Sources/Haverkort_1990_Ecology of potato cropping systems in relation to latitude and altitude.pdf}
}

