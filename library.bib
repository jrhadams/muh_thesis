@incollection{Bradshaw2021,
  title = {Increasing {{Potato Yields}}: {{A Conundrum}}},
  shorttitle = {Increasing {{Potato Yields}}},
  booktitle = {Potato {{Breeding}}: {{Theory}} and {{Practice}}},
  author = {Bradshaw, John E.},
  editor = {Bradshaw, John E.},
  date = {2021},
  pages = {125--193},
  publisher = {Springer International Publishing},
  location = {Cham},
  doi = {10.1007/978-3-030-64414-7_3},
  url = {https://doi.org/10.1007/978-3-030-64414-7_3},
  urldate = {2025-08-02},
  abstract = {Increasing yield per hectare is an obvious breeding objective for potatoes, whether as a staple food for local consumption, as a vegetable for cash or as a crop for processing into food or starch. Selection is for high fresh-weight yield, at an appropriate dry-matter content for the end use, in assessment environments appropriate for the target length and nature of the growing season and agronomy inputs. As a quantitative trait, yield is assessed in field trials with due consideration of interference between plots and genotype × environment interactions. Combining ability analysis indicates adequate genetic variation for yield increases over generations that have proved difficult to achieve in practice (a conundrum). Consideration is therefore given to harvest index, tuberization in different day-lengths, ideotypes, eco-physiological models, path coefficient analysis, current gaps between actual and potential yields, tolerance of abiotic stresses such as drought, heat and salt, and nitrogen and phosphorous use efficiency.},
  isbn = {978-3-030-64414-7},
  langid = {english}
}

@article{Bradshaw2022,
  title = {A {{Brief History}} of the {{Impact}} of {{Potato Genetics}} on the {{Breeding}} of {{Tetraploid Potato Cultivars}} for {{Tuber Propagation}}},
  author = {Bradshaw, John E.},
  date = {2022-09},
  journaltitle = {Potato Research},
  volume = {65},
  number = {3},
  pages = {461--501},
  issn = {1871-4528},
  doi = {10.1007/s11540-021-09517-w},
  url = {https://link.springer.com/article/10.1007/s11540-021-09517-w},
  urldate = {2023-05-04},
  abstract = {The principal cultivated potato (Solanum tuberosum) has mainly been vegetatively propagated through its tubers. Potato breeders have therefore made planned artificial hybridizations to generate genetically unique seedlings and their clonal descendants from which to select new cultivars for tuber propagation. After the initial hybridizations, no more sexual reproduction was required to produce a successful new cultivar, which depended on choosing the correct breeding objectives and the ability to recognize a clone that met those objectives. Any impact of the new science of genetics after 1900 needed to be through the production of parental material of known genetic constitution and predictable offspring. This included making use of the many wild tuber-bearing relatives of the potato in Central and South America, as well as the abundance of landraces in South America. This review looks at the history of how potato geneticists: 1) established that the principal cultivated potato is a tetraploid that displays tetrasomic inheritance (2n = 4x = 48); 2) developed progeny tests to determine the dosage of major genes for qualitative traits in potential parents, and also progeny tests for their general combining abilities for quantitative traits; and 3) provided molecular markers for the marker assisted selection of major genes and quantitative trait alleles of large effect, and for the genomic selection of many alleles of small effect. It is argued that the concepts of population genetics are required by breeders, once a number of cycles of hybridization and cultivar production are considered for the genetic improvement of potato crops.},
  langid = {english},
  file = {/home/jrha/Sources/Bradshaw_2022_A Brief History of the Impact of Potato Genetics on the Breeding of Tetraploid.pdf}
}

