  author = {Wegman, E.J. and Wright, I.W.},
  date = {1983},
  journaltitle = {Journal of the American Statistical Association},
  volume = {78},
  number = {382},
  pages = {351--365},
  abstract = {This is a survey article that attempts to synthesize a broad variety of work on splines in statistics. Splines are pre- sented as a nonparametric function estimating technique. After a general introduction to the theory of interpolating and smoothing splines, splines are treated in the nonpar- ametric regression setting. The method of cross-valida- tion for choosing the smoothing parameter is discussed and the general multivariate regression/surface estima- tion problem is addressed. An extensive discussion of splines as nonparametric density estimators is followed by a discussion of their role in time series analysis. A comparison of the spline and isotonic regression meth- odologies leads to a formulation of a hybrid estimator. The closing section provides a brief overall summary and formulates a number of open/unsolved problems relating to splines in statistics.},
  file = {/home/jrha/Zotero/storage/ZAWWGQBH/m-api-7845cfbe-b927-3a01-2e2c-830f5dcd10f4.pdf}
}

