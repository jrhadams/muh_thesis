@article{Rowe1967,
  title = {Performance and Variability of Diploid and Tetraploid Potato Families},
  author = {Rowe, P. R.},
  date = {1967-08},
  journaltitle = {American Potato Journal},
  volume = {44},
  number = {8},
  pages = {263--271},
  publisher = {Springer-Verlag},
  issn = {00030589},
  doi = {10.1007/BF02862526},
  url = {https://link.springer.com/article/10.1007/BF02862526},
  urldate = {2021-01-29},
  abstract = {The performance and variability of 11 diploid and 11 tetraploid potato hybrid families were investigated during two growing seasons at Sturgeon Bay, Wisconsin. The tetraploid families were significantly higher yielding than the diploids when grown from true seed but the differences were reduced when the materials were grown from tubers. The tetraploids were taller and had more stems 4 weeks after planting, and produced more and heavier tubers over 1.5 inches in diameter. There was no difference in total tuber number or weight. The diploid hybrids usually did not perform as well as their parental clones, while the tetraploids not only exceeded the mean of their parents but regularly exceeded the higher parent. In diploid and tetraploid potatos, gene action and heterozygosity may have a greater effect on plant vigor and productivity than ploidy level. The diploid families generally had more within-population variability. This provides evidence that diploid potatoes will respond to selection faster than tetraploids. © 1967 Springer.},
  keywords = {Agriculture,Plant Breeding/Biotechnology,Plant Genetics and Genomics,Plant Pathology,Plant Sciences},
  file = {/home/jrha/Zotero/storage/KSBGZBFG/full-text.pdf}
}

