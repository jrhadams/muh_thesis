@article{Moeinizade2019,
  title = {Optimizing {{Selection}} and {{Mating}} in {{Genomic Selection}} with a {{Look-Ahead Approach}}: {{An Operations Research Framework}}},
  author = {Moeinizade, Saba and Hu, Guiping and Wang, Lizhi and Schnable, Patrick S},
  date = {2019},
  doi = {10.1534/g3.118.200842},
  url = {https://doi.org/10.1534/g3.118.200842},
  abstract = {New genotyping technologies have made large amounts of genotypic data available for plant breeders to use in their efforts to accelerate the rate of genetic gain. Genomic selection (GS) techniques allow breeders to use genotypic data to identify and select, for example, plants predicted to exhibit drought tolerance, thereby saving expensive and limited field-testing resources relative to phenotyping all plants within a population. A major limitation of existing GS approaches is the trade-off between short-term genetic gain and long-term potential. Some approaches focus on achieving short-term genetic gain at the cost of reduced genetic diversity necessary for long-term gains. In contrast, others compromise short-term progress to preserve long-term potential without consideration of the time and resources required to achieve it. Our contribution is to define a new "look-ahead" metric for assessing selection decisions, which evaluates the probability of achieving high genetic gains by a specific time with limited resources. Moreover , we propose a heuristic algorithm to identify optimal selection decisions that maximize the look-ahead metric. Simulation results demonstrate that look-ahead selection outperforms other published selection methods. KEYWORDS},
  file = {/home/jrha/Sources/Moeinizade et al_2019_Optimizing Selection and Mating in Genomic Selection with a Look-Ahead Approach.pdf}
}

