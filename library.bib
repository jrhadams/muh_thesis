  file = {/home/jrha/Sources/Scott_Suarez_2012_THE RISE OF ASIA AS THE CENTRE OF GLOBAL POTATO PRODUCTION AND SOME.pdf}
}

@article{Scutari2014,
  title = {No {{Title}}},
  author = {Scutari, Marco and Howell, Phil and Balding, David J. and Mackay, Ian},
