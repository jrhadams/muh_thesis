@article{Gopal2015,
  title = {Challenges and {{Way-forward}} in {{Selection}} of {{Superior Parents}}, {{Crosses}} and {{Clones}} in {{Potato Breeding}}},
  author = {Gopal, J.},
  date = {2015-06},
  journaltitle = {Potato Research},
  volume = {58},
  number = {2},
  pages = {165--188},
  issn = {1871-4528},
  doi = {10.1007/s11540-015-9292-6},
  url = {https://link-springer-com.ezproxy.library.wur.nl/article/10.1007/s11540-015-9292-6},
  urldate = {2023-01-20},
  abstract = {The success of any potato breeding programme depends upon the selection of superior parents and cross combinations, and superior clones from the segregating progenies. However, due to the tetrasomic inheritance and heterozygous nature of cultivated potato, potato breeding has remained empirical, cumbersome and time-consuming. Potato breeders have used a number of approaches to simplify and improve the efficiency of potato breeding. These include use of mid-parent/mid-self values, genetic diversity, combining ability, estimated breeding values and progeny tests for the selection of parents and cross combinations. The selection of superior clones from the segregating progeny has been hampered by low efficiency of selection in early generations particularly for tuber yield and its components. The possibility of in vitro selection and production of microtubers for early large scale trials has been explored. The procedures used and constraints observed are reviewed and discussed in this paper.},
  langid = {english},
  file = {/home/jrha/Sources/Gopal_2015_Challenges and Way-forward in Selection of Superior Parents, Crosses and Clones.pdf}
}

