@article{Coudert2010,
  title = {Genetic Control of Root Development in Rice, the Model Cereal},
  author = {Coudert, Yoan and Périn, Christophe and Courtois, Brigitte and Khong, Ngan Giang and Gantet, Pascal},
  date = {2010-04},
  journaltitle = {Trends in Plant Science},
