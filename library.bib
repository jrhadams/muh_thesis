@article{Atkin2009,
  title = {Impact of Depth of Pedigree and Inclusion of Historical Data on the Estimation of Additive Variance and Breeding Values in a Sugarcane Breeding Program},
  author = {Atkin, Felicity Claire and Dieters, Mark J. and Stringer, Joanne K.},
  date = {2009-08},
  journaltitle = {Theoretical and Applied Genetics},
  volume = {119},
  number = {3},
  pages = {555--65},
  issn = {0040-5752},
  doi = {10.1007/s00122-009-1065-7},
  url = {https://www.proquest.com/docview/204512319/abstract/FCCC789F3E694EE9PQ/1},
  urldate = {2024-12-08},
  abstract = {Sugarcane breeders in Australia combine data across four selection programs to obtain estimates of breeding value for parents. When these data are combined with full pedigree information back to founding parents, computing limitations mean it is not possible to obtain information on all parents. Family data from one sugarcane selection program were analysed using two different genetic models to investigate how different depths of pedigree and amount of data affect the reliability of estimating breeding value of sugarcane parents. These were the parental and animal models. Additive variance components and breeding values estimated from different amounts of information were compared for both models. The accuracy of estimating additive variance components and breeding values improved as more pedigree information and historical data were included in analyses. However, adding years of data had a much larger effect on the estimation of variance components of the population, and breeding values of the parents. To accurately estimate breeding values of all sugarcane parents, a minimum of three generations of pedigree and 5 years of historical data were required, while more information (four generations of pedigree and 7 years of historical data) was required when identifying top parents to be selected for future cross pollination.[PUBLICATION ABSTRACT]},
  langid = {english},
  keywords = {Accuracy,Breeding program,Estimates,Sugarcane},
  file = {/home/jrha/Sources/Atkin et al_2009_Impact of depth of pedigree and inclusion of historical data on the estimation.pdf}
}

