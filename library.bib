@article{Sun2025,
  title = {The Phased Pan-Genome of Tetraploid {{European}} Potato},
  author = {Sun, Hequan and Tusso, Sergio and Dent, Craig I. and Goel, Manish and Wijfjes, Raúl Y. and Baus, Lisa C. and Dong, Xiao and Campoy, José A. and Kurdadze, Ana and Walkemeier, Birgit and Sänger, Christine and Huettel, Bruno and Hutten, Ronald C. B. and family=Eck, given=Herman J., prefix=van, useprefix=true and Dehmer, Klaus J. and Schneeberger, Korbinian},
  date = {2025-06},
  journaltitle = {Nature},
  volume = {642},
  number = {8067},
  pages = {389--397},
  publisher = {Nature Publishing Group},
  issn = {1476-4687},
  doi = {10.1038/s41586-025-08843-0},
  url = {https://www.nature.com/articles/s41586-025-08843-0},
  urldate = {2025-08-16},
  abstract = {Potatoes were first brought to Europe in the sixteenth century1,2. Two hundred\,years later, one of the species had become one of the most important food sources across the entire continent and, later, even the entire world3. However, its highly heterozygous, autotetraploid genome has complicated its improvement since then4–7. Here we present the pan-genome of European potatoes generated from phased genome assemblies of ten historical potato cultivars, which includes approximately 85\% of all haplotypes segregating in Europe. Sequence diversity between the haplotypes was extremely high (for example, 20× higher than in humans), owing to numerous introgressions from wild potato species. By contrast, haplotype diversity was very low, in agreement with the population bottlenecks caused by domestication and transition to Europe. To illustrate a practical application of the pan-genome, we converted it into a haplotype graph and used it to generate phased, megabase-scale pseudo-genome assemblies of commercial potatoes (including the famous French fries potato ‘Russet Burbank’) using cost-efficient short reads only. In summary, we present a nearly~complete pan-genome of autotetraploid European potato, we describe extraordinarily high sequence diversity in a domesticated crop, and we outline how this resource might be used to accelerate genomics-assisted breeding and research.},
  langid = {english},
  keywords = {Agricultural genetics,Genetic variation,Genome informatics,Haplotypes,Plant genetics},
  file = {/home/jrha/Zotero/storage/CJ5KTIMQ/Sun et al. - 2025 - The phased pan-genome of tetraploid European potato.pdf}
}

