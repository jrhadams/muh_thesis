@article{Shang2016,
  title = {Main Effect {{QTL}} with Dominance Determines Heterosis for Dynamic Plant Height in Upland Cotton},
  author = {Shang, Lianguang and Ma, Lingling and Wang, Yumei and Su, Ying and Wang, Xiaocui and Li, Yuhua and Abduweli, Abdugheni and Cai, Shihu and Liu, Fang and Wang, Kunbo and Hua, Jinping},
  date = {2016},
  journaltitle = {G3: Genes, Genomes, Genetics},
  volume = {6},
  number = {10},
  pages = {3373--3379},
  publisher = {Genetics Society of America},
  issn = {21601836},
  doi = {10.1534/g3.116.034355},
  abstract = {Plant height, which shows dynamic development and heterosis, is a major trait affecting plant architecture and has an indirect influence on economic yield related to biological yield in cotton. In the present study, we carried out dynamic analysis for plant height and its heterosis by quantitative trait loci (QTL) mapping at multiple developmental stages using two recombinant inbred lines (RILs) and their backcross progeny. At the single-locus level, 47 QTL were identified at five developmental stages in two hybrids. In backcross populations, QTL identified at an early stage mainly showed partial effects and QTL detected at a later stage mostly displayed overdominance effects. At the two-locus level, we found that main effect QTL played a more important role than epistatic QTL in the expression of heterosis in backcross populations. Therefore, this study implies that the genetic basis of plant height heterosis shows dynamic character and main effect QTL with dominance determines heterosis for plant height in Upland cotton.},
  keywords = {Backcross population,Heterosis,Plant height,QTL,Upland cotton},
  file = {/home/jrha/Zotero/storage/ARMLS4V9/full-text.pdf}
}

