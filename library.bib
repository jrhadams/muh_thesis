@article{Wang2011,
  title = {Enrichment of a Common Wheat Genetic Map and {{QTL}} Mapping for Fatty Acid Content in Grain},
  author = {Wang, Ying-ying and Sun, Xian-yin and Zhao, Yan and Kong, Fan-mei and Guo, Ying and Zhang, Gui-zhi and Pu, Yan-yan and Wu, Ke and Li, Si-shen},
  date = {2011-07},
  journaltitle = {Plant Science},
