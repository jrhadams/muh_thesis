@article{Chakhlevitch,
  title = {Minimising the {{Number}} of {{Gap-Zeros}} in {{Binary Matrices}}},
  author = {Chakhlevitch, Konstantin and Glass, Celia A and Shakhlevich, Natalia V},
  journaltitle = {European Journal of Operational Research},
  volume = {229},
  number = {1},
  issn = {0377-2217},
  doi = {10.1016/j.ejor.2013.01.028},
  url = {http://eprints.whiterose.ac.uk/79240/},
  urldate = {2019-10-23},
  abstract = {We study a problem of minimising the total number of zeros in the gaps between blocks of consecutive ones in the columns of a binary matrix by permuting its rows. The problem is refered to as the Consecutive One Augmentation Problem, and is known to be NP-hard. An analysis of the structure of an optimal solution allows us to focus on a restricted solution space, and to use an implicit representation for searching the space. We develop an exact solution algorithm, fixed-parameter tractable with respect to the number of columns, and two constructive heuristics to tackle instances with an arbitrary number of columns. The heuristics use a novel solution representation based upon row sequencing. In our computational study, all heuristic solutions are either optimal or close to an optimum. One of the heuristics is particularly effective, especially for problems with a large number of rows.},
  keywords = {Binary matrices,Combinatorial optimisation,Consecutive ones property,Heuristics,Scheduling}
}

