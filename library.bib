@article{Souza2020,
  title = {Adaptability and Stability Analyses of Plants Using Random Regression Models},
  author = {family=Souza, given=Michel Henriques, prefix=de, useprefix=false and Júnior, José Domingos Pereira and Steckling, Skarlet De Marco and Mencalha, Jussara and Dias, Fabíola dos Santos and Rocha, João Romero do Amaral Santos de Carvalho and Carneiro, Pedro Crescêncio Souza and Carneiro, José Eustáquio de Souza},
  date = {2020-12-02},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
  volume = {15},
  number = {12},
  pages = {e0233200},
  publisher = {Public Library of Science},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0233200},
  url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0233200},
  urldate = {2025-05-30},
  abstract = {The evaluation of cultivars using multi-environment trials (MET) is an important step in plant breeding programs. One of the objectives of these evaluations is to understand the genotype by environment interaction (GEI). A method of determining the effect of GEI on the performance of cultivars is based on studies of adaptability and stability. Initial studies were based on linear regression; however, these methodologies have limitations, mainly in trials with genetic or statistical unbalanced, heterogeneity of residual variances, and genetic covariance. An alternative would be the use of random regression models (RRM), in which the behavior of the genotypes is characterized as a reaction norm using longitudinal data or repeated measurements and information regarding a covariance function. The objective of this work was the application of RRM in the study of the behavior of common bean cultivars using a MET, based on Legendre polynomials and genotype-ideotype distances. We used a set of 13 trials, which were classified as unfavorable or favorable environments. The results revealed that RRM enables the prediction of the genotypic values of cultivars in environments where they were not evaluated with high accuracy values, thereby circumventing the unbalanced of the experiments. From these values, it was possible to measure the genotypic adaptability according to ideotypes, according to their reaction norms. In addition, the stability of the cultivars can be interpreted as variation in the behavior of the ideotype. The use of ideotypes based on real data allowed a better comparison of the performance of cultivars across environments. The use of RRM in plant breeding is a good alternative to understand the behavior of cultivars in a MET, especially when we want to quantify the adaptability and stability of genotypes.},
  langid = {english},
  keywords = {Animal sexual behavior,Beans,Covariance,Genetic polymorphism,Genetics,Phenotypes,Plant breeding,Polynomials},
  file = {/home/jrha/Zotero/storage/C6I3WDPB/Souza et al. - 2020 - Adaptability and stability analyses of plants using random regression models.pdf}
}

