@article{George2017,
  title = {Climate {{Change}} and {{Consequences}} for {{Potato Production}}: A {{Review}} of {{Tolerance}} to {{Emerging Abiotic Stress}}},
  shorttitle = {Climate {{Change}} and {{Consequences}} for {{Potato Production}}},
  author = {George, Timothy S. and Taylor, Mark A. and Dodd, Ian C. and White, Philip J.},
  date = {2017-12-01},
  journaltitle = {Potato Research},
  shortjournal = {Potato Res.},
  volume = {60},
  number = {3},
  pages = {239--268},
  publisher = {Springer Netherlands},
  issn = {1871-4528},
  doi = {10.1007/s11540-018-9366-3},
  url = {https://link.springer.com/article/10.1007/s11540-018-9366-3},
  urldate = {2025-05-30},
  abstract = {Potato is a major global crop that has an important role to play in food security, reducing poverty and improving human nutrition. Enhanced atmospheric CO2 concentrations provide an opportunity to increase potato yields in the future, but this will only be possible if the potato crop can cope with the other consequences of climate change caused by this rise in CO2. While climate change may impact biotic stress either positively or negatively, abiotic stresses are likely to be greatly increased and become a major threat to potato production. Increasing heat, drought and salinity stress will drive the need for greater understanding of genes, traits and management techniques that allow potato to cope with these stresses. In this review, we identify some of the key physiological and molecular adaptations of potato to these stresses and propose an ideotype which should include (1) optimal stomatal regulation to balance water loss and heat stress in leaves, (2) production of metabolites and transporters to scavenge reactive oxygen species and partition toxic elements, (3) enhanced root systems to maximise water capture, (4) maintenance of tuberisation under stress conditions and (5) stress avoidance by accelerating crop development and reducing time to yield. We discuss potential ways to achieve this ideotype, emphasising the need to benefit from genetic diversity in landrace and wild material by screening for traits in combined stress environments appropriate to future agroecosystems.},
  issue = {3},
  langid = {english},
  file = {/home/jrha/Zotero/storage/YHFJQHGQ/George et al. - 2017 - Climate Change and Consequences for Potato Production a Review of Tolerance to Emerging Abiotic Str.pdf}
}

