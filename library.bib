@article{Kusmec2021,
  title = {Interdisciplinary Strategies to Enable Data-Driven Plant Breeding in a Changing Climate},
  author = {Kusmec, Aaron and Zheng, Zihao and Archontoulis, Sotirios and Ganapathysubramanian, Baskar and Hu, Guiping and Wang, Lizhi and Yu, Jianming and Schnable, Patrick S.},
  date = {2021-03},
  journaltitle = {One Earth},
  volume = {4},
  number = {3},
  pages = {372--383},
  issn = {25903322},
  doi = {10.1016/j.oneear.2021.02.005},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S259033222100110X},
  urldate = {2024-03-06},
  abstract = {This perspective lays out a framework to enable the breeding of crops that can meet worldwide demand under the challenges of global climate change. Past work in various fields has produced multiple prediction methods to contribute to different plant breeding objectives. Our proposed framework focuses on the integration of these methods into decision-support tools that quantify the effects on multiple objectives of decisions made throughout the plant breeding pipeline. We discuss the complementarities among these methods with an emphasis on integration into tools that utilize operations research and systems approaches to help plant breeders rapidly and optimally design new cultivars under extant time, cost, and environmental constraints. In illustrating this potential, we demonstrate the interconnectedness and probabilistic nature of plant breeding objectives and highlight research opportunities to refine and combine knowledge across multiple disciplines. Such a framework can help plant breeders more efficiently breed for future environments, including so-called minor crops, leading to an overall increase in the resiliency of global food production systems.},
  langid = {english},
  file = {/home/jrha/Sources/Kusmec et al_2021_Interdisciplinary strategies to enable data-driven plant breeding in a changing.pdf}
}

