@article{DeVries2023,
  title = {Converting {{Hybrid Potato Breeding Science}} into {{Practice}}},
  author = {De Vries, Michiel E. and Adams, James R. and Eggers, Ernst-jan and Ying, Su and Stockem, Julia E. and Kacheyo, Olivia C. and Van Dijk, Luuk C. M. and Khera, Pawan and Bachem, Christian W. and Lindhout, Pim and Van Der Vossen, Edwin A. G.},
  date = {2023-01},
  journaltitle = {Plants},
  volume = {12},
  number = {2},
  pages = {230},
  issn = {2223-7747},
  doi = {10.3390/plants12020230},
  url = {https://www.mdpi.com/2223-7747/12/2/230},
  urldate = {2024-06-03},
  abstract = {Research on diploid hybrid potato has made fast advances in recent years. In this review we give an overview of the most recent and relevant research outcomes. We define different components needed for a complete hybrid program: inbred line development, hybrid evaluation, cropping systems and variety registration. For each of these components the important research results are discussed and the outcomes and issues that merit further study are identified. We connect fundamental and applied research to application in a breeding program, based on the experiences at the breeding company Solynta. In the concluding remarks, we set hybrid breeding in a societal perspective, and we identify bottlenecks that need to be overcome to allow successful adoption of hybrid potato.},
  langid = {english},
  file = {/home/jrha/Sources/De Vries et al_2023_Converting Hybrid Potato Breeding Science into Practice.pdf}
}

